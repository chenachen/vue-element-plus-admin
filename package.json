{
  "name": "vue-element-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "npx eslint ./src --ext .vue,.js,.ts",
    "eslint:fix": "npx eslint ./src --ext .vue,.js,.ts --fix",
    "stylelint": "npx stylelint \"src/**/*.vue\" \"src/**/*.less\"",
    "stylelint:fix": "npx stylelint --fix \"src/**/*.vue\" \"src/**/*.less\"",
    "pre-commit": "npm run ts-check && npm run test && lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.1.2",
    "element-plus": "^2.3.6",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.3",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "stylelint": "^15.7.0",
    "typescript": "5.0.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  }
}
